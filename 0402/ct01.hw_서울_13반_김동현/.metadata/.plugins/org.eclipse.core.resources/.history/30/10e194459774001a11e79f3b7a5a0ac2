package algo.hw0402;

import java.util.Scanner;

public class SWEA_6782_D5_현주가좋아하는제곱근놀이 {

	static long N;
	
	static boolean isSqrt(long n) {
		double temp= Math.sqrt(n);
		
		if(temp==(long)temp) {
			return true;
		}
		return false;
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		
		int tc=sc.nextInt();
		
		for (int t = 0; t < tc; t++) {
			N=sc.nextLong();
			
			StringBuilder sb= new StringBuilder();
			sb.append("#").append(t+1).append(" ");
			
			
			int cnt=0;
			double temp;
			while(N!=2) {
				
//				if(isSqrt(N)) {
//					temp=Math.sqrt(N);
//					N=(long)temp;
//				}else {
//					N=N+1;
//				}
//				cnt++;
				
				temp=Math.sqrt(N);
				long temp2= (long)temp;
				
				if(N==temp2*temp2) {
					N=temp2;
					cnt++;
					
				}else {
					
					long n2=temp2*temp2+(temp2-1)+3;
					cnt+=n2-N;
					N=n2;
					
				}
				
			}
			sb.append(cnt);
			
			System.out.println(sb.toString());

		}
		
	}

}
